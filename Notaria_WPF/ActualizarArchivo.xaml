<Window x:Class="Notaria_WPF.ActualizarArchivo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Notaria_WPF"
        mc:Ignorable="d"
        Title="ActualizarArchivo" Height="139" Width="284" Icon="/Img/logo.ico">
    <Grid Margin="0,0,0,-6">
        <TextBox x:Name="nombreTxt" HorizontalAlignment="Left" Margin="78,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="148" Height="22"/>
        <Label  Content="Nombre" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold" Height="34" Width="80" RenderTransformOrigin="0.525,0.805"/>
        <Button x:Name="modificarBtn" Content="Modificar" HorizontalAlignment="Left" Margin="157,87,0,0" VerticalAlignment="Top" Height="24" Width="101" Click="subirArchivo_Click"/>
        <Button x:Name="cerrarBtn" Content="Cerrar" HorizontalAlignment="Left" Margin="33,87,0,0" VerticalAlignment="Top" Height="24" Width="101" Click="cerrarBtn_Click"/>
        <Border BorderBrush="Black" BorderThickness="1" Margin="-4,-1,0,0" CornerRadius="4"/>
        <Label Content="Ingrese nombre a modificar" HorizontalAlignment="Left" Margin="31,-1,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="16"/>
    </Grid>
</Window>
